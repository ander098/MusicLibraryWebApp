@model List<MusicLibraryWebApp.Models.Song>

<h1>Album: @ViewBag.Album</h1>
<ul class="list-group">
    @foreach (var song in Model)
    {
        <li class="list-group-item">
            <a href="/MusicLibrary/SongDetails?title=@song.Title">@song.Title</a> by @song.Artist
        </li>
    }
</ul>
<a href="/MusicLibrary" class="btn btn-secondary mt-3">Back to Library</a>
